<%= turbo_frame_tag dom_id(project, :favorite) do %>
  <%= button_to project_favorites_path(project), method: :post do %>
    <svg xmlns="http://www.w3.org/2000/svg" class="h-10 w-10 text-red-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
      <%= tag.path 'stroke-linecap':'round', 'stroke-linejoin':'round', d:emmbed_svg('favorite')['path']%>
    </svg>
  <% end %>
<% end %>